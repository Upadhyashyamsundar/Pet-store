<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>Dogs</title>
  <meta charset="utf-8">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/petsStyle.css">
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=JetBrains+Mono&family=Lobster&family=Varela+Round&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Dancing+Script:wght@700&display=swap" rel="stylesheet">
  </head>

  <header>
    <h1 class="headerName">Currently Avilable Breeds...</h1>
  </header>

  <body>
    

    <% Dogs.forEach(function(dog){  %>
      
         <form action="/dogsCustomers/<%=dog.Did%>">

          
              <div class="col-4">

               <h1 name="breed" class="breedName"><%= dog.Dbreed %></h1>
               <img class="dogImg"  src="/images/dogs/<%=dog.img%>" alt="puppy">
               <div class="dogInfo">
                <p>Colour - <%= dog.Dcolor %></p>
                 <p>Age - <%= dog.Dage %> Weeks</p>
                  <p>Price - â‚¹<%= dog.Dprice %></p>
                </div>
                <button class="btn btn-danger" type="submit">Buy now!</button> 
                <hr>
                
              </div>

            

        

          

        </form>
          

        
        <% }) %>
       
 </body>

